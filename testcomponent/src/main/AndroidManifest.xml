<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xuanfeng.testcomponent">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.TestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.TestAIDLActivity" />
        <activity android:name=".activity.TestShareAnimActivity" />

        <activity android:name=".activity.TestTouchEventActivity" />

        <service
            android:name=".service.TestService"
            android:enabled="true"
            android:exported="true"></service>

        <activity android:name=".activity.TestForEditTextActivity" />
        <activity android:name=".activity.TestForGalleryActivity" />
    </application>

</manifest>